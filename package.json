    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "@material-ui/core": "^4.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "name": "react-mui-dialog",
  "author": "Andrew Ross",
  "keywords": [
    "react",
    "reactjs",
    "hooks",
    "typescript",
    "material-ui",
    "dialog",
    "formik",
    "yup"
  ],
  "module": "dist/react-mui-dialog.esm.js",
  "size-limit": [
    {
      "path": "dist/react-mui-dialog.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-mui-dialog.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@material-ui/core": "^4.11.3",
    "@size-limit/preset-small-lib": "^4.10.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "husky": "^5.1.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "size-limit": "^4.10.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3",
    "yup": "^0.32.9"
  },
  "dependencies": {
    "formik": "^2.2.6",
    "formik-material-ui": "^3.0.1",
    "lodash": "^4.17.21"
  }
